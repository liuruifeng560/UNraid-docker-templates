  <Config Name="App Name" Target="UI_APPNAME" Default="" Mode="" Description="Home Name" Type="Variable" Display="always" Required="false" Mask="false">&#x5C0F;&#x8FEA; PDF</Config>
  <Config Name="Home Description" Target="UI_HOMEDESCRIPTION" Default="" Mode="" Description="Home Description" Type="Variable" Display="always" Required="false" Mask="false">&#x5C0F;&#x8FEA;&#x7684; PDF &#x5DE5;&#x574A;</Config>
  <Config Name="Navbar Name" Target="UI_APPNAMENAVBAR" Default="" Mode="" Description="Navbar Name" Type="Variable" Display="always" Required="false" Mask="false">&#x5C0F;&#x8FEA; PDF</Config>
  <Config Name="Google Visibility" Target="SYSTEM_GOOGLEVISIBILITY" Default="false|true" Mode="" Description="Google Visibility" Type="Variable" Display="always" Required="false" Mask="false">false</Config>
  <Config Name="Custom Static File Path" Target="SYSTEM_CUSTOMSTATICFILEPATH" Default="" Mode="" Description="Custom Static File Path" Type="Variable" Display="always" Required="false" Mask="false">/customFiles/static</Config>
