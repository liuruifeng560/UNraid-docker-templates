  <Config Name="Config Path" Target="/configs" Default="" Mode="rw" Description="Config Path" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/StirlingPDF/configs</Config>
  <Config Name="Enable Login" Target="SECURITY_ENABLELOGIN" Default="true|false" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="CSRF Disabled" Target="SECURITY_CSRFDISABLED" Default="true|false" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <Config Name="Docker Enable Security" Target="DOCKER_ENABLE_SECURITY" Default="false|true" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">false</Config>
  <Config Name="System Connection Timeout Minutes" Target="SYSTEM_CONNECTIONTIMEOUTMINUTES" Default="1|3|5" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1</Config>
  <Config Name="System Root URI Path" Target="SYSTEM_ROOTURIPATH" Default="/" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">/</Config>
  <Config Name="Metrics" Target="METRICS_ENABLED" Default="true|false" Mode="" Description="" Type="Variable" Display="advanced" Required="false" Mask="false">true</Config>
  <Config Name="Endpoints To Remove" Target="ENDPOINTS_TOREMOVE" Default="" Mode="" Description="e.g. ['img-to-pdf', 'remove-pages']" Type="Variable" Display="advanced" Required="false" Mask="false">[]</Config>
  <Config Name="Endpoints Groups To Remove" Target="ENDPOINTS_GROUPSTOREMOVE" Default="" Mode="" Description="e.g. ['LibreOffice']" Type="Variable" Display="advanced" Required="false" Mask="false">[]</Config>
